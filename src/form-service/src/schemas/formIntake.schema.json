{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FormIntake",
  "type": "object",
  "required": [
    "id",
    "intakeId",
    "templateId",
    "insuranceLine",
    "customerId",
    "status",
    "formDataRaw"
  ],
  "properties": {
    "id": { "type": "string" },
    "intakeId": { "type": "string" },
    "templateId": { "type": "string" },
    "insuranceLine": { "type": "string" },
    "customerId": { "type": "string" },
    "status": { "enum": ["draft", "completed"] },
    "formDataRaw": { "type": "object", "additionalProperties": true },
    "formDataNormalized": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": true
      }
    },
    "isRenewal": { "type": "boolean" },
    "sourceEventId": { "type": "string" }
  }
}

