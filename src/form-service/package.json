{
  "name": "",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "test": "npm run build && node --test \"dist/src/tests/**/*.js\"",
    "test:watch": "tsx watch --test \"src/tests/**/*.test.ts\"",
    "test:coverage": "npm run build && node --test --experimental-test-coverage \"dist/src/tests/**/*.js\"",
    "precommit": "npm test",
    "verify": "npm run test:coverage",
    "migrate:connectors": "tsx scripts/migrateConnectorConfigs.ts",
    "migrate:connectors:live": "tsx scripts/migrateConnectorConfigs.ts --live"
  },
  "dependencies": {
    "@azure/cosmos": "^4.8.0",
    "@azure/functions": "^4.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "jsonata": "^2.1.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "azure-functions-core-tools": "^4.x",
    "husky": "^9.0.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.7.0",
    "typescript": "^4.0.0"
  }
}
